{
  "name": "field-tool-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "supabase:init": "supabase init",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "db:migrate": "supabase migration new",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset && NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321 SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6InNlcnZpY2Vfcm9sZSIsImV4cCI6MTk4MzgxMjk5Nn0.EGIM96RAZx35lJzdJsyH-qQwv8Hdp7fsn3W0YpN81IU tsx scripts/create-test-users.ts",
    "db:seed": "tsx supabase/seed.ts",
    "seed:dev": "PGPASSWORD=postgres psql -h localhost -p 54322 -U postgres -d postgres -f scripts/seed-simple.sql",
    "create:users": "PGPASSWORD=postgres psql -h localhost -p 54322 -U postgres -d postgres -f scripts/create-users.sql",
    "seed:all": "npm run seed:dev && npm run create:users",
    "seed:invoices": "NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321 SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6InNlcnZpY2Vfcm9sZSIsImV4cCI6MTk4MzgxMjk5Nn0.EGIM96RAZx35lJzdJsyH-qQwv8Hdp7fsn3W0YpN81IU tsx scripts/create-test-data-invoices.ts",
    "check:env": "tsx scripts/check-env.ts",
    "check:all": "npm run lint && npm run type-check && npm run check:env",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "backup": "tsx scripts/auto-backup.ts",
    "prepare": "husky install",
    "build:search-index": "tsx scripts/build-search-index.ts"
  },
  "dependencies": {
