    "nodemailer": "^7.0.11",
    "papaparse": "^5.5.3",
    "pdf-lib": "^1.17.1",
    "pdfmake": "^0.2.20",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "rehype-autolink-headings": "^7.1.0",
