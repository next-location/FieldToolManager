    "@testing-library/react": "^14.1.2",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.16.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@types/stripe": "^8.0.416",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.6.1",
